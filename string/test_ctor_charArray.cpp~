// test.cpp for String ctor char Array

#include<iostream>
#include<cassert>
#include"string.hpp"

int main(){

  //Setup

  //Test
  char a[]="abc";
  String test(a);

  //Verify
  std::cout<<"abc=="<<test<<std::endl;
  assert(test==a);
  std::cout<<"Done testing char array ctor"<<std::endl;

}
